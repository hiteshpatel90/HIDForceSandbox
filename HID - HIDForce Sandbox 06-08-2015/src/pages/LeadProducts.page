<apex:page standardController="Lead" extensions="LeadProductController" showheader="false" sidebar="false" language="{!language}" >
    <link rel="stylesheet" type="text/css" href="{!$Resource.leadProductsCSS}" />
    <script type='text/javascript'>
        /* var:termsLang, is a value (case sensitive) passed as a query parameter through the url of this page
         * it is used to determine which hard coded terms and conditions to show
        **/
        var englishTermsDiv = 'en_usTermsDiv';
        var language = '{!language}';
        var termsId = language+'TermsDiv';
        function showTerms(){
            var promoForm = document.getElementById('leadFormDiv');
            var termsDiv = (document.getElementById(termsId) != null) ? document.getElementById(termsId) : document.getElementById(englishTermsDiv);
            termsDiv.style.display='block';
            promoForm.style.visibility='hidden';
        }
        function hideTerms(){
            var promoForm = document.getElementById('leadFormDiv');
            var termsDiv = (document.getElementById(termsId) != null) ? document.getElementById(termsId) : document.getElementById(englishTermsDiv);
            termsDiv.style.display='none';
            promoForm.style.visibility='visible';
        }
        function changeLocation(redirectUrl){
            var submitted = document.getElementById('hdnLeadSubmitted').value;
            if (submitted == "true") {
                window.top.location = redirectUrl;
            }
        }
    </script>
    <div id="leadFormDiv">
        <apex:form id="leadForm" >
            <apex:actionFunction action="{!insertLead}" name="insertLead" oncomplete="changeLocation('{!redirectUrl}');" rerender="contactInfoPanel, leadProductPanel" />
            <apex:outputPanel id="leadFormPanel">
            <h2>{!$ObjectType.Lead.fields.Contact_Information_translation__c.label}</h2>
                        
            <apex:outputPanel id="contactInfoPanel">

            <apex:outputPanel id="theMessages" layout="block" rendered="{!hasErrorMessages}" >
                <ul class="apex_err_msgs">
                    <apex:repeat value="{!customErrorMessages}" var="anError" id="theRepeat">
                        <li> {!anError} </li>
                    </apex:repeat>
                </ul>
            </apex:outputPanel>
            
                <table cellpadding="2" cellspacing="2" border="0" width="100%" >
                    <tbody>
                        <tr class="ltGrey">
                            <td>{!$ObjectType.Lead.fields.FirstName.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.FirstName}" required="true" /></td>
                        </tr>
                        <tr class="dkGrey">
                            <td>{!$ObjectType.Lead.fields.LastName.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.LastName}" required="true" /></td>
                        </tr>
                        <tr class="ltGrey">
                            <td>{!$ObjectType.Lead.fields.Company.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.Company}" required="true" /></td>
                        </tr>
                        <tr class="dkGrey">
                            <td>{!$ObjectType.Lead.fields.Title.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.Title}" required="true" /></td>
                        </tr>
                        <tr class="ltGrey">
                            <td>{!$ObjectType.Lead.fields.Country.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.Country}" /></td>
                        </tr>
                        <tr class="dkGrey">
                            <td>{!$ObjectType.Lead.fields.Street.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.Street}" required="true" /></td>
                        </tr>
                        <tr class="ltGrey">
                            <td>{!$ObjectType.Lead.fields.City.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.City}" required="true" /></td>
                        </tr>
                        <tr class="dkGrey">
                            <td>{!$ObjectType.Lead.fields.State.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.State}" required="true" /></td>
                        </tr>
                        <tr class="ltGrey">
                            <td>{!$ObjectType.Lead.fields.PostalCode.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.PostalCode}" required="true" /></td>
                        </tr>
                        <tr class="dkGrey">
                            <td>{!$ObjectType.Lead.fields.Email.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.Email}" required="true" /></td>
                        </tr>
                        <tr class="ltGrey">
                            <td>{!$ObjectType.Lead.fields.Phone.label}<span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!leadRecord.Phone}" required="true" /></td>
                        </tr>
                        <tr class="dkGrey">
                            <td>
                                <span id="acceptTxt">
                                    <a href="#" onclick="showTerms();" >
                                    <!-- <a href="#" onclick="changeLocation({!leadSubmitted}, '{!redirectUrl}');" > -->
                                    
                                         {!$ObjectType.Lead.fields.Terms_and_Conditions__c.label}
                                    </a>
                                </span>
                                <span style="color:#cc0000;">*</span>
                            </td>
                            <td>
                                <apex:outputPanel id="acceptCheckBox" >
                                    <span id="inlineCheckBox" >
                                        <apex:inputField value="{!leadRecord.Terms_and_Conditions__c}" required="true" />
                                        <span style="display:inline;">
                                            {!$ObjectType.Lead.fields.I_accept__c.label}
                                        </span>
                                    </span>
                                </apex:outputPanel>
                            </td>
                        </tr>
                </tbody>
            </table>
            </apex:outputPanel>
            
            <h2>{!$ObjectType.Lead.fields.Product_Informationtranslate__c.label}</h2>
            <apex:outputPanel layout="block" id="leadProductPanel">
                <apex:actionRegion >
                <table cellpadding="2" cellspacing="2" border="0" width="100%">
                    <tr class="ltGrey">
                    <td></td>
                    <td width="25%">{!$ObjectType.Lead_Products__c.fields.Model_of_integrated_HID_Component__c.label}</td>
                    <td width="25%">{!$ObjectType.Lead_Products__c.fields.Part_of_Integrated_HID_Component__c.label}</td>
                    <td width="25%">{!$ObjectType.Lead_Products__c.fields.Model_Containing_HID_Component__c.label}</td>
                    <td></td>
                    </tr>
                    
                    <apex:repeat value="{!leadProductsList}" var="wrapper">
                        <tr class="{!IF( MOD(wrapper.rowIndex, 2) == 0 , 'ltGrey', 'dkGrey')}" >
                            <td><apex:outputLabel value="{!wrapper.rowIndex}"/><span style="color:#cc0000;">*</span></td>
                            <td><apex:inputField value="{!wrapper.leadProduct.Model_of_integrated_HID_Component__c}" required="true" /></td>
                            <td><apex:inputField value="{!wrapper.leadProduct.Part_of_Integrated_HID_Component__c}" required="true" /></td>
                            <td><apex:inputField value="{!wrapper.leadProduct.Model_Containing_HID_Component__c}" required="true" /></td>
                            <td>
                            <!------------------------------------------------
                                We only want to display the Add More button
                                on the last row of the table.  So, if the
                                index on the wrapper is equal to the total 
                                number of rows, display the button, otherwise
                                hide it
                            ------------------------------------------------->
                                <apex:outputPanel rendered="{!AND((wrapper.rowIndex == totalNumberOfProductsAdded), (totalNumberOfProductsAdded > 1))}" >
                                    <apex:actionSupport action="{!handleRemoveProduct}" event="onclick" rerender="leadProductPanel" immediate="true" />
                                    <input type="button" value="{!$ObjectType.Lead.fields.Remove__c.label}" />
                                </apex:outputPanel>
                                
                                <apex:outputPanel rendered="{!(wrapper.rowIndex == totalNumberOfProductsAdded)}" >
                                    <apex:actionSupport action="{!handleAddMoreRequest}" event="onclick" rerender="leadProductPanel" />
                                    <input type="button" value="{!$ObjectType.Lead.fields.Add_MoreButton__c.label}" />
                                </apex:outputPanel>
                            </td>
                        </tr>
                    </apex:repeat>
                </table>
                </apex:actionRegion>
                <input type="hidden" value="{!leadSaved}" id="hdnLeadSubmitted" />
            </apex:outputPanel>
            
            <apex:outputPanel styleClass="submitContainer" layout="block" id="buttonPanel"  >
                <input type="button" value="{!$ObjectType.Lead.fields.Submitbutton__c.label}" onclick="javascript:insertLead();"/>
            </apex:outputPanel>
            
            </apex:outputPanel>
        </apex:form>
    </div> 
