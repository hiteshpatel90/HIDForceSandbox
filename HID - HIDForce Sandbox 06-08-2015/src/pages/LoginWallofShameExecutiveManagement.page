<apex:page controller="retrieveLastLoginGroup">
        <apex:dataTable value="{!executivemanagement}" var="e" style="width:100%;text-align:left;">
            <apex:column value="{!e.firstname} {!e.lastname}" headerValue="User Name" style="text-align:left;"/>
            <apex:column value="{!e.lastlogindate}" headerValue="Last Login" style="text-align:right;width:120px;"/>
        </apex:dataTable>
</apex:page>