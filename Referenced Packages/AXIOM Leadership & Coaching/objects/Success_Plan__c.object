    <validationRules>
        <fullName>ClosingRatioRule</fullName>
        <active>true</active>
        <description>Closing Ratio and Proposal ratio can NOT be &gt; 100%.</description>
        <errorConditionFormula>Closing_Ratio__c &gt; 100</errorConditionFormula>
        <errorDisplayField>Closing_Ratio__c</errorDisplayField>
        <errorMessage>Closing Ratio can not be &gt; 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndDateValidation</fullName>
        <active>false</active>
        <description>End Date cannot be in past.</description>
        <errorConditionFormula>ISCHANGED( End_Date__c ) &amp;&amp; (End_Date__c &lt; TODAY()) &amp;&amp; (TEXT(Status__c)  = &apos;Active&apos;)</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End Date cannot be in past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProposalRatioRule</fullName>
        <active>true</active>
        <description>Closing Ratio and Proposal ratio can NOT be &gt; 100%.</description>
        <errorConditionFormula>Proposal_Ratio__c &gt; 100</errorConditionFormula>
        <errorDisplayField>Proposal_Ratio__c</errorDisplayField>
        <errorMessage>Proposal ratio can NOT be &gt; 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDateValidation</fullName>
        <active>false</active>
        <errorConditionFormula>AND((MONTH(Start_Date__c) &lt;  MONTH(TODAY())),(YEAR(Start_Date__c) &lt;  YEAR(TODAY())),(DAY(Start_Date__c) &lt;  DAY(TODAY()))) &amp;&amp; (TEXT(Status__c)  = &apos;Active&apos;)</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>Start Date cannot be earlier than 12 months ago</errorMessage>
    </validationRules>
