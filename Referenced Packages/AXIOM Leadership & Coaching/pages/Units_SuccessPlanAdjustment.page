	var adjustBar = document.getElementById('adj-bar-graph-inner');

	var plan = parseFloat(removecomma($("[id*=annualSalesplan]").text()));
	var predicted = parseFloat(removecomma($("[id*=annualSales_plan_pred]").text()));
	var actual = parseFloat(removecomma($("[id*=annualSales_plan_hist]").text()));
	var adjusted = parseFloat(removecomma($("#annS_T").text()));

	$("[id*=actualAmount]").text(' '+accounting.formatNumber(actual, 0, separator1, separator2));
	$("[id*=goalAmount]").text(' '+accounting.formatNumber(plan, 0, separator1, separator2));
	$("#adjActual  b").text( accounting.formatNumber(predicted, 0, separator1, separator2) +" / ");
	$("#adjGoal ").text( accounting.formatNumber(plan, 0, separator1, separator2));
	$("#adjActual_cal  b").text( accounting.formatNumber(adjusted, 0, separator1, separator2) +" / ");
	$("#adjGoal_cal ").text( accounting.formatNumber(plan, 0, separator1, separator2));

	var max = plan;
	if(actual>max) max = actual;
	if(predicted>max) max = predicted;
	if(adjusted>max) max = adjusted;

	var actualPerc = (actual/max)*100;
	var predictedPerc = predicted/max*100;
	var adjustedPerc = adjusted/max*100;
	var goalPerc = plan/max*100;



	predictedGoal.style["width"]=goalPerc+"%";
	adjustGoal.style["width"]=goalPerc+"%";
	predictedActual.style["width"]=(actualPerc > 100 ? 100: actualPerc)+"%";
	adjustActual.style["width"]=(actualPerc > 100 ? 100: actualPerc)+"%";

	predictedBar.style["width"]=(predictedPerc > 100 ? 100: predictedPerc)+"%";
	adjustBar.style["width"]= (adjustedPerc > 100  ? 100: adjustedPerc==100? 100:adjustedPerc)+"%";

	/***************************************************/
	predictedBar.className ="bar-graph-inner yellow";
	adjustBar.className ="bar-graph-inner green";
	/*adjusted >= plan ?
