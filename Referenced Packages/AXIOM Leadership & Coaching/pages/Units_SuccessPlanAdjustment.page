<apex:page standardController="AXIOM_LC__Success_Plan__c" extensions="AXIOM_LC.Units_SuccessPlanAdjustment" id="adjustmentPage" showHeader="false" sideBar="false">
	<!-- <apex:messages /> -->
	<link href="{!URLFOR($Resource.lc_css_component, 'lc_css_base.css')}" rel="stylesheet" />
	<link href="{!URLFOR($Resource.lc_css_component, 'lc_css_component.css')}" rel="stylesheet" />

	<apex:includeScript value="{!URLFOR($Resource.AXIOM_LC__lc_boot_component_templates, 'jquery/js/jquery-1.9.1.js')}" />
	<apex:includeScript value="{!URLFOR($Resource.AXIOM_LC__lc_boot_component_templates, 'jquery/js/jquery-ui-1.10.3.custom.min.js')}" />
	<apex:includeScript value="{!URLFOR($Resource.AXIOM_BASE__accounting)}" />
	<apex:stylesheet value="{!URLFOR($Resource.AXIOM_LC__lc_boot_component_templates, 'jquery/css/ui-lightness/jquery-ui-1.10.3.custom.css')}" />
	<link
		href="{!URLFOR($Resource.lc_boot_component, './css/axiom-coaching.css')}"
		rel="stylesheet" />

	<script src="{!URLFOR($Resource.lc_boot_component_templates, 'underscore.js')}"></script>

	<style media="screen" type="text/css">
.disabledSlider {
	position: absolute;
	z-index: 66666;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	background-color: rgba(0, 0, 0, 0.1);
}

.irs {
	overflow: hidden;
}

input[type="text"] {
	border: medium none;
	padding-top: 0;
	text-align: left;
	background: inherit;
}

#annS,#avg,#cRat,#pRat,#opp {
	display: none;
}

.highlight {
	color: green;
	font-weight: bold;
}
</style>
	<div class="page-header">
		<div class="page-heading left">
			<!-- {!$Label.SP_AdjustSuccessPlan} -->
			{!SuccessPlan.SuccessPlan_Name__c} *
			<br/>
		</div>
		<div class="btn-container right">
			<input type="button" id="save-button"
				class="button primary left {!IF(isEditable,'','disabled')}"
				value="{!$Label.Btn_Save}" />
			<div class="clear"></div>
		</div>
		<div class="clear"></div>
	</div>
	<div class="page-content">
		<div class="table-adjust" style="margin-right: 0;">
			<apex:form >
				<div id="form-confirm-submit" style="display: none;">
					<p>Are you sure to update the Current Plan with your
						adjustments?</p>
					<apex:commandLink styleClass="button primary right"
						style="text-decoration:none; " action="{!Save}" value="Ok"
						id="saveButton" />
				</div>
				<div id="form-alert-submit" style="display: none;">
					<p>Adjusted Annualized Units Objective should be same as the Plan
						Units Objective. Please adjust other metrics using the Sliders to
						adjust the Annualized Units Objective.</p>
				</div>
				<table class="table table-striped">
					<thead>
						<tr>
							<th>{!$Label.axiom_lc__SP_Item}</th>
							<th>{!$Label.axiom_lc__SP_Plan}</th>
							<th>{!$Label.axiom_lc__SP_Actual}</th>
							<th>{!$Label.axiom_lc__SP_Predicted}</th>
							<th>{!$Label.axiom_lc__SP_PeerAverage}</th>
							<th>{!$Label.axiom_lc__SP_Adjusted}</th>
							<th>{!$Label.axiom_lc__Pin}</th>
							<th>Move the slide bar to adjust the plan</th>
						</tr>
					</thead>
					<tbody>
						<tr>
							<td>Annualized Units Objective</td>
							<td><apex:outputText value="{!SuccessPlanWrp.Annual_Units_Objective}"
									id="annualSalesplan" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpHistorical.Annual_Units_Objective}"
									id="annualSales_plan_hist" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPredicted.Annual_Units_Objective}"
									id="annualSales_plan_pred" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPeerAverage.Annual_Units_Objective}"
									id="annualSales_plan_peer" /></td>
							<td><span id="annS_T">{!SuccessPlanWrpAdjusted.Annual_Units_Objective}</span>
								<apex:inputHidden value="{!SuccessPlan.AXIOM_LC__Annual_Units__c}" /></td>
							<td><input type="radio" name="pinGroup" value="1" /></td>
							<td class="ui-slider-td">
								<div style="position: relative;">
									<div id="slider-annual-sales" class="sliderClass">
										<div class="ui-slider-start-point"></div>
										<span class="ui-slider-legend">0%</span>
									</div>
									<div class="ui-value-right">0%</div>
								</div> <!-- <input id="slider-annual-sales" class="sliderClass" type="hidden" value="-100;100" /> -->
							</td>
						</tr>
						<tr>
							<td>{!$Label.SP_Periods}</td>
							<td><apex:outputText value="{!SuccessPlanWrp.Number_of_Periods}" id="periods_plan" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpHistorical.Number_of_Periods}"
									id="periods_plan_hist" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPredicted.Number_of_Periods}"
									id="periods_plan_pred" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPeerAverage.Number_of_Periods}"
									id="periods_plan_peer" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpAdjusted.Number_of_Periods}"
									id="periods_plan_adj" /></td>
							<td>&nbsp;</td>
							<td>&nbsp;</td>
						</tr>
						<tr>
							<td>Units per period</td>
							<td><apex:outputText value="{!SuccessPlanWrp.Units_Per_Period}"
									id="perPeriod_plan" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpHistorical.Units_Per_Period}"
									id="perPeriod_plan_hist" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPredicted.Units_Per_Period}"
									id="perPeriod_plan_pred" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPeerAverage.Units_Per_Period}"
									id="perPeriod_plan_peer" /></td>
							<td><apex:outputText value="{!SuccessPlanWrp.Units_Per_Period}"
									id="perPeriod_plan_adj" /></td>
							<td>&nbsp;</td>
							<td>&nbsp;</td>
						</tr>
						<tr>
							<td>Avg. Num. of Units</td>
							<td><apex:outputText value="{!SuccessPlanWrp.Avg_Num_Of_Units}" id="avg_plan" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpHistorical.Avg_Num_Of_Units}"
									id="avg_plan_hist" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPredicted.Avg_Num_Of_Units}"
									id="avg_plan_pred" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPeerAverage.Avg_Num_Of_Units}"
									id="avg_plan_peer" /></td>
							<td><span id="avg_T">{!SuccessPlanWrpAdjusted.Avg_Num_Of_Units}</span>
								<apex:inputHidden value="{!SuccessPlan.AXIOM_LC__Avg_Num_Of_Units__c}" /></td>
							<td><input type="radio" name="pinGroup" value="2" /></td>
							<td class="ui-slider-td">
								<div style="position: relative;">
									<div id="slider-avg-sales" class="sliderClass">
										<div class="ui-slider-start-point"></div>
										<span class="ui-slider-legend">0%</span>
									</div>
									<div class="ui-value-right">0%</div>
								</div> <!-- <input id="slider-avg-sales" class="sliderClass" type="hidden" value="-100;100" /> -->
							</td>
						</tr>
						<tr>
							<td>{!$Label.SP_ClosesPerPeriod}</td>
							<td><apex:outputText value="{!SuccessPlanWrp.Closes_Per_Period}"
									id="closesPerPeriod_plan" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpHistorical.Closes_Per_Period}"
									id="closesPerPeriod_plan_hist" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPredicted.Closes_Per_Period}"
									id="closesPerPeriod_plan_pred" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPeerAverage.Closes_Per_Period}"
									id="closesPerPeriod_plan_peer" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpAdjusted.Closes_Per_Period}"
									id="closesPerPeriod_plan_adj" /></td>
							<td>&nbsp;</td>
							<td>&nbsp;</td>
						</tr>
						<tr>
							<td>{!$Label.SP_ClosingRatio}</td>
							<td><apex:outputText value="{!SuccessPlanWrp.Closing_Ratio}"
									id="closingRatio_plan" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpHistorical.Closing_Ratio}"
									id="closingRatio_plan_hist" /></td>
							<td><apex:outputText value="{!SuccessPlanWrppredicted.Closing_Ratio}"
									id="closingRatio_plan_pred" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPeerAverage.Closing_Ratio}"
									id="closingRatio_plan_peer" /></td>
							<td><span id="cRat_T">{!SuccessPlanWrpAdjusted.Closing_Ratio}</span>
								<apex:inputHidden value="{!SuccessPlan.AXIOM_LC__Closing_Ratio__c}" /></td>
							<td><input type="radio" name="pinGroup" value="3" /></td>
							<td class="ui-slider-td">
								<div style="position: relative;">
									<div id="slider-closing-ratio" class="sliderClass">
										<div class="ui-slider-start-point"></div>
										<span class="ui-slider-legend">0%</span>
									</div>
									<div class="ui-value-right">0%</div>
								</div> <!-- <input id="slider-closing-ratio" class="sliderClass" type="hidden" value="-100;100" /> -->
							</td>
						</tr>
						<tr>
							<td>{!$Label.SP_ProposalsPerPeriod}</td>
							<td><apex:outputText value="{!SuccessPlanWrp.New_Proposals_per_Period}"
									id="proposalsPerPeriod_plan" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpHistorical.New_Proposals_per_Period}"
									id="proposalsPerPeriod_plan_hist" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPredicted.New_Proposals_per_Period}"
									id="proposalsPerPeriod_plan_pred" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPeerAverage.New_Proposals_per_Period}"
									id="proposalsPerPeriod_plan_peer" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpAdjusted.New_Proposals_per_Period}"
									id="proposalsPerPeriod_plan_adj" /></td>
							<td>&nbsp;</td>
							<td>&nbsp;</td>

						</tr>
						<tr>
							<td>{!$Label.SP_ProposalRatio}</td>
							<td><apex:outputText value="{!SuccessPlanWrp.Proposal_Ratio}"
									id="proposalsRatio_plan" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpHistorical.Proposal_Ratio}"
									id="proposalsRatio_plan_hist" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPredicted.Proposal_Ratio}"
									id="proposalsRatio_plan_pred" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPeerAverage.Proposal_Ratio}"
									id="proposalsRatio_plan_peer" /></td>
							<td><span id="pRat_T">{!SuccessPlanWrpAdjusted.Proposal_Ratio}</span>
								<apex:inputHidden value="{!SuccessPlan.AXIOM_LC__Proposal_Ratio__c}" /></td>
							<td><input type="radio" name="pinGroup" value="4" /></td>
							<td class="ui-slider-td">
								<div style="position: relative;">
									<div id="slider-proposal-ratio" class="sliderClass">
										<div class="ui-slider-start-point"></div>
										<span class="ui-slider-legend">0%</span>
									</div>
									<div class="ui-value-right">0%</div>
								</div> <!-- <input id="slider-proposal-ratio" class="sliderClass"  type="hidden" value="-100;100" /> -->
							</td>
						</tr>
						<tr>
							<td>{!$Label.SP_NewOpportunitiesPerPeriod}</td>
							<td><apex:outputText value="{!SuccessPlanWrp.New_Opportunities_per_Period}"
									id="opportunitiesPerPeriod_plan" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpHistorical.New_Opportunities_per_Period}"
									id="opportunitiesPerPeriod_plan_hist" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPredicted.New_Opportunities_per_Period}"
									id="opportunitiesPerPeriod_plan_pred" /></td>
							<td><apex:outputText value="{!SuccessPlanWrpPeerAverage.New_Opportunities_per_Period}"
									id="opportunitiesPerPeriod_plan_peer" /></td>
							<td><span id="opp_T">{!SuccessPlanWrpAdjusted.New_Opportunities_per_Period}</span>
								<apex:inputHidden value="{!SuccessPlan.AXIOM_LC__New_Opportunities_per_Period__c}" /></td>
							<td><input type="radio" name="pinGroup" value="5" /></td>
							<td class="ui-slider-td">
								<div style="position: relative;">
									<div id="slider-new-opportunities" class="sliderClass">
										<div class="ui-slider-start-point"></div>
										<span class="ui-slider-legend">0%</span>
									</div>
									<div class="ui-value-right">0%</div>
								</div> <!-- <input id="slider-new-opportunities" class="sliderClass" type="hidden" value="-100;100" /> -->
							</td>
						</tr>
					</tbody>
				</table>
			</apex:form>
		</div>
		<div class="clear"></div>
		<div class="bar-graphs">
			<div class="bar-graph-container">
				<label class="adjust adjust-left">{!$Label.SP_Predicted}</label> <label
					class="adjust adjust-right" id="adjustRight"><span
					id="adjActual"><b>$750,000/</b></span><span id="adjGoal">$1,200,000</span></label>
				<div class="bar-graph adjust">
					<div class="bar-graph-goal" style="width: 0%;"
						id="act-bar-graph-goal"></div>
					<div class="bar-graph-actual" style="width: 0%;"
						id="act-bar-graph-actual">
						<div class="bar-graph-actual-label-container">
							<label class="bar-graph-actual-label"> <apex:outputText value="" id="actualAmount">{!$Label.axiom_lc__SP_Actual}: </apex:outputText>
							</label>
						</div>
					</div>
					<div class="bar-graph-inner yellow" style="width: 0%;"
						id="act-bar-graph-inner"></div>
				</div>
			</div>
			<div class="bar-graph-container">
				<label class="adjust adjust-left">{!$Label.SP_Adjusted}</label> <label
					class="adjust adjust-right"><span id="adjActual_cal"><b>$750,000/</b></span><span
					id="adjGoal_cal">$1,200,000</span></label>
				<div class="bar-graph adjust">
					<div class="bar-graph-goal" style="width: 0%;"
						id="adj-bar-graph-goal">
						<div class="bar-graph-goal-label-container">
							<label class="bar-graph-goal-label"> <apex:outputText value="" id="goalAmount">{!$Label.axiom_lc__SP_Goal}: </apex:outputText>
							</label>
						</div>
					</div>
					<div class="bar-graph-actual" style="width: 0%;"
						id="adj-bar-graph-actual"></div>
					<div class="bar-graph-inner yellow" style="width: 0%;"
						id="adj-bar-graph-inner"></div>
				</div>
			</div>
		</div>
	</div>
	<apex:outputPanel layout="none" rendered="{!isEditable || isSeller}">
		<div id="chatter">
			<chatter:feed entityId="{!SuccessPlan.Id}"/>
		</div>
	</apex:outputPanel>
	<apex:outputPanel layout="none" rendered="{!isSeller}">
	<script type="text/javascript">
		$(document).ready(function(){
			$('.publishercontainer').remove();
		});
	</script>
	<style>
		.publishercontainer{display:none;}
	</style>
	</apex:outputPanel>

