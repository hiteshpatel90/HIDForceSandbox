				adjustBar.className ="bar-graph-inner yellow":adjustBar.className ="bar-graph-inner red";
	predicted >= plan ?
				predictedBar.className ="bar-graph-inner green":
				(predicted>=plan*50/100 && predicted<plan)?
				predictedBar.className ="bar-graph-inner yellow":predictedBar.className ="bar-graph-inner red";*/
}

