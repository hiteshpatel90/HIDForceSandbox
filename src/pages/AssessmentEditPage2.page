<apex:page standardController="Assessment__c" extensions="assessmentEditController" tabStyle="Assessment__c">
    <script>
        var asmtStatus = "{!Assessment__c.Approval_Status__c}";
        var profileName = "{!$Profile.Name}";
        if(profileName != "System Administrator"){
            if(asmtStatus != 'Draft' && asmtStatus != 'Rejected' && asmtStatus != ''){
                alert("The Record is Locked, Please Contact Your System Administrator for Assistance");
                window.location = "https://{!serverURL}{!returnURL}";
            }
        }
        
        var accountId = "{!Assessment__c.Account__c}";
        var accountClusterId = "{!Assessment__c.Account_Cluster__c}";
        var bolAccountClusterOwnerEditorMatch = {!bolAccountClusterOwnerEditorMatch};
        if(accountId == '' && accountClusterId == ''){
            alert("No Account/Account Cluster selected. Please open an Account/Account Cluster and create the Assessment there.");
            window.location = "https://{!serverURL}{!returnURL}";
        }else if(accountClusterId != '' && bolAccountClusterOwnerEditorMatch == false){
            alert("Only Account Cluster Owner can create/edit Account Cluster Assessments.");
            window.location = "https://{!serverURL}{!returnURL}";
        }
    </script>
    <c:assessmentHeader assessmentName="{!objAssessment.Name}" mode="Edit"/>
    <apex:form >
        <apex:pageMessages />
        <apex:pageBlock title="Assessment" mode="edit">
            <apex:pageBlockButtons >
                <apex:commandButton action="{!updateAssessment}" value="Update Assessment"/>
                <apex:commandButton action="{!step1}" value="Back"/>
                <apex:commandButton action="{!customCancel}" value="Cancel"/>
                <apex:commandButton action="{!customSave}" value="Save"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection title="Account Information" columns="2">
                <apex:outputField value="{!objAssessment.Account__c}" rendered="{!bolAccount}"/>
                <apex:outputField value="{!objAssessment.Account_Cluster__c}" rendered="{!IF(bolAccount,false,true)}"/>
                <apex:outputField value="{!objAssessment.Business_Segment__c}"/>
                <apex:outputField value="{!objAssessment.Market_Size__c}"/>
                <apex:outputField value="{!objAssessment.Partner_Type__c}"/>
                <apex:outputText value="{!strRegion}" label="Region"/>
                <apex:inputField value="{!objAssessment.Approval_Status__c}"/>
                <apex:pageBlockSectionItem helptext="{!$ObjectType.Assessment__c.fields.Partner_Tier_Notification_Contact__c.inlineHelpText}">
                	<apex:outputLabel value="{!$ObjectType.Assessment__c.fields.Partner_Tier_Notification_Contact__c.label}" for="lblPartnerTierNotificationContact"/>
	                <apex:outputPanel layout="block" styleClass="requiredInput">
	                        <apex:outputPanel layout="block" styleClass="requiredBlock"/>
	                		<apex:inputField value="{!Assessment__c.Partner_Tier_Notification_Contact__c}" id="lblPartnerTierNotificationContact"/>
	                </apex:outputPanel>
	            </apex:pageBlockSectionItem>
            </apex:pageBlockSection>
