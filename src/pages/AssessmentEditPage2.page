                        </apex:actionStatus>
                        <apex:inputField value="{!objAssessment.SI_Spend__c}" styleClass="inputText">
                            <apex:actionSupport action="{!updateAssessment}" event="onchange" reRender="frmScoring" status="imgSISpend" focus="SI_Software_Spend__c"/>
