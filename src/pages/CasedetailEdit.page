        <script>       
            function setChannelPartAcc(pklst,pklstid,txtaccName){ 
                           
                if(pklst.value == 'Direct'){
                    document.getElementById(pklstid).removeAttribute("disabled");
                    document.getElementById(pklstid+'_lkwgt').style.visibility = "visible";
                    //document.getElementById(pklstid).value = document.getElementById(txtaccName).innerHTML;
                       
                    callsetAccountName(pklst.value);                    
                }else if(pklst.value == 'Channel Partner'){
                    document.getElementById(pklstid).value = "";
                    //document.getElementById(pklstid).setAttribute("required");
                    document.getElementById(pklstid).removeAttribute("disabled");
                    document.getElementById(pklstid+'_lkwgt').style.visibility = "visible";
                }else if(pklst.value == 'Unknown'){
                    document.getElementById(pklstid).value = "";
                    document.getElementById(pklstid).disabled = "true";
                    //document.getElementById(pklstid+'_lkwgt').href = "#";
                     document.getElementById(pklstid+'_lkwgt').style.visibility = "hidden";
                    return false;
                }              
            }
            function setChannelPartAccload(pklst,pklstid,txtaccName){ 
                if(pklst != null){           
                    if(pklst.value == 'Direct'){
                        document.getElementById(pklstid).removeAttribute("disabled");                   
                        document.getElementById(pklstid+'_lkwgt').style.visibility = "visible";
                        //document.getElementById(pklstid).value = document.getElementById(txtaccName).innerHTML;
                       
                    }else if(pklst.value == 'Channel Partner'){             
                        document.getElementById(pklstid).removeAttribute("disabled");
                        document.getElementById(pklstid+'_lkwgt').style.visibility = "visible";
                    }else if(pklst.value == 'Unknown'){
                        document.getElementById(pklstid).value = "";
                        document.getElementById(pklstid).disabled = "true";
                        //document.getElementById(pklstid+'_lkwgt').href = "#";
                         document.getElementById(pklstid+'_lkwgt').style.visibility = "hidden";
                        return false;
                    }      
                }        
            }
            <!--  pivCLASS  -->
            function validateChannelPartnerpivClass(){                             
                var pklst = document.getElementById('{!$Component.pbMainpivClass:pbsFirst:pbitemPurchasepath:selPurchasePath}');
                if(pklst.value == 'Channel Partner' && '{!isCustService}' == 'false'){                   
                    var cpavalue = document.getElementById('{!$Component.pbMainpivClass:pbsFirst:pbsItemCPA:txtCPA}').value;
                    if(cpavalue == null || cpavalue == ''){
                        alert("Please select Channel Partner account");                    
                        return false;                     
                    }else{                       
                        return true;
                    }
                }else{                    
                    return true;
                }                
            }
            <!--  Printer/Asure ID/EasyLobby -->
            function validateChannelPartnerprinter(){                             
                var pklst = document.getElementById('{!$Component.pbMainprinter:pbsFirst:pbitemPurchasepath:selPurchasePath}');
                if(pklst.value == 'Channel Partner' && '{!isCustService}' == 'false'){                   
                    var cpavalue = document.getElementById('{!$Component.pbMainprinter:pbsFirst:pbsItemCPA:txtCPA}').value;
                    if(cpavalue == null || cpavalue == ''){
                        alert("Please select Channel Partner account");                    
                        return false;                     
                    }else{                       
                        return true;
                    }
                }else{                    
                    return true;
                }                
            }
            <!--  Readers/Cards/Network Products/HID  -->
            function validateChannelPartnerreader(){                             
                var pklst = document.getElementById('{!$Component.pbMainreader:pbsFirst:pbitemPurchasepath:selPurchasePath}');
                if(pklst.value == 'Channel Partner' && '{!isCustService}' == 'false'){                   
