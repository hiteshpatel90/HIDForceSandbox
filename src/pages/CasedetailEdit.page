                    }else{                       
                        return true;
                    }
                }else{                    
                    return true;
                }                
            }
            
            function setAccountName(){
                if(pklstcpa != null){
                    callsetAccountName(pklstcpa.value);
                }else{
                    callsetAccountName('');
                }                     
            }
            
            function showProblemCodeFunc(drdVal){
                if(drdVal.value != ''){
                    var drdValue = drdVal.value;
                    callshowProblemCode(drdValue);
                }
