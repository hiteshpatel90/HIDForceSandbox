<apex:page controller="retrieveLastLoginGroup">
        <apex:dataTable value="{!marketing}" var="m" style="width:100%;text-align:left;">
            <apex:column value="{!m.firstname} {!m.lastname}" headerValue="User Name" style="text-align:left;"/>
            <apex:column value="{!m.lastlogindate}" headerValue="Last Login" style="text-align:right;width:120px;"/>
        </apex:dataTable>
</apex:page>